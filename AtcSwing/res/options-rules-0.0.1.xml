<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE digester-rules PUBLIC "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" "digester-rules.dtd" >
<digester-rules>
	<pattern value="options">
		<object-create-rule classname="org.mmarini.atc.xml.UserOptions" />
		<pattern value="hits/record">
			<object-create-rule
				classname="org.mmarini.atc.sim.GameRecord" />
			<set-properties-rule />
			<set-next-rule methodname="addRecord" />
		</pattern>
	</pattern>
</digester-rules>
